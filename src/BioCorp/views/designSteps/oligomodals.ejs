<!-- Modal for File Upload-->
<div class="modal fade bd-example-modal-lg" id="oligoFileUploadModal" tabindex="-1" role="dialog">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title"><%=texts.upload_oligo_file%></h4>
      </div>
      <form action="/bulkOligoAddToCart" method="post">
        <div class="modal-body">
          <input type="hidden" name="bulkOligo" id="bulkOligo" value=""/>
          <table id="oligoUploadTable" class="table table-striped">
          </table>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal"><%=texts.close%></button>
          <button type="submit" class="btn btn-primary" id="bulkOligoAddToCart"><%=texts.add_cart%></button>
        </div>
      </form>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

<!-- Modal for Regions for Target Design-->
<div class="modal fade" id="oligoSequenceModal" role="dialog">
  <div class="modal-dialog">

    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title"><%=texts.regular_bases%></h4>
      </div>
      <div class="modal-body">
        <p><%=texts.enter_sequences%></p>
      </div>

      <div class="modal-header">
        <h4 class="modal-title"><%=texts.mixed_bases%></h4>
      </div>
      <div class="modal-body">
        <p><%=texts.mixed_bases_desc%></p>
      </div>

      <div class="modal-header">
        <h4 class="modal-title">Phosphorothioate</h4>
      </div>
      <div class="modal-body">
        <p><%=texts.phosphorothioate_desc%></p>
      </div>

      <div class="modal-header">
        <h4 class="modal-title">5′ or 3′ <%=texts.modification%></h4>
      </div>
      <div class="modal-body">
        <p><%=texts.modification_desc%></p>
      </div>

      <div class="modal-header">
        <h4 class="modal-title"><%=texts.length%></h4>
      </div>
      <div class="modal-body">
        <p><%=texts.length_desc%></p>
      </div>

      <div class="modal-footer">
        <button type="button" class="btn btn-default action-button" name="button" data-dismiss="modal"><%=texts.close%></button>
      </div>
    </div>

  </div>
</div>

